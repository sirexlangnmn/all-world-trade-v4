<%- include('../includes/head/index.ejs') %>

<body>
  <div id="wrapper" class="is-collapse is-active">
    <!-- === HEADER - START === -->
    <%- include('../includes/header/index.ejs') %>
    <!-- === HEADER - END === -->

    <!-- Main Contents -->
    <div class="main_content_custom">
    
      <!-- === REGISTRATION NAV - START === -->
      <%- include('../includes/registration/registration_nav.ejs') %>
      <!-- === REGISTRATION NAV - END === -->

      <div class="mcontainer">
        <form action="javascript:void(0);" method="POST" id="lookingForMediumScaleCompanyForm" name="lookingForMediumScaleCompanyForm" enctype="multipart/form-data" accept-charset="utf-8">
          <div class="profile is_page">
            <div class="profiles_banner profile_avatar image-upload">
              <label for="companyBanner">
                <img id="companyBannerPreview" src="uploads/placeholder/banner-placeholder2.jpg" />
              </label>
              <input type="file" id="companyBanner" name="companyBanner" />
            </div>
            <p class="text-red-500" style="text-align: center;" id="companyBannerValidation"></p>
            <div class="profiles_content">
              <div class="profile_avatar image-upload">
                <label for="companyLogo">
                  <img id="companyLogoPreview" src="uploads/placeholder/logo-placeholder2.jpg" />
                </label>
                <input type="file" id="companyLogo" name="companyLogo" />
                <p class="text-red-500" style="text-align: center; margin-top: -18px" id="companyLogoValidation"></p>
              </div>
              <div class="profile_info">
                <h1>Members Looking For Medium Scale Company</h1>
                <h1>(Individual and Company Information)</h1>
              </div>
            </div>
            <nav class="responsive-nav border-t -mb-0.5 lg:pl-2">
              <ul>
                <li class="active" id=""><a href="#" id="">About</a></li>
                <li><a href="#0">Media Files </a></li>
                <li><a href="#0">Reviews</a></li>
                <li><a href="#0">Discussion</a></li>
                <li><a href="#0">Videos</a></li>
                <li><a href="#0">Dashboard</a></li>
                <li><a href="#0">Settings</a></li>
              </ul>
            </nav>
          </div>


          <div class="lg:flex lg:space-x-10 mt-2.5">
            <!-- ===============================  -->
            <!-- Company Details Display - START  -->
            <!-- ===============================  -->
            <div class="lg:w-2/4 w-full mb-2">
              <div class="bg-white rounded-md lg:shadow-md shadow">
                <div class="border-b flex items-center justify-between p-4">
                  <div>
                    <h2 class="text-lg font-semibold">Company Details
                      <a class="hover:border-gray-100" href="#">
                        <span class="icon-feather-info" style="font-size: 16px; font-family: inherit" uk-tooltip="title: Company Details Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat. Duis aute irure dolor in reprehenderit in voluptate velit esse cillum dolore eu fugiat nulla pariatur. Excepteur sint occaecat cupidatat non proident, sunt in culpa qui officia deserunt mollit anim id est laborum.; pos: right">
                        </span>
                      </a>
                    </h2>
                  </div>
                  <div class="text-right">
                  </div>
                </div>
                <div class="flex-1 space-y-4 p-6">
                  <div id="new">
                    <label for=""> Company Name: </label>
                    <input type="text" placeholder="" class="shadow-none with-border" id="companyName" name="companyName">
                    <p class="text-red-500" id="companyNameValidation"></p>
                  </div>
                  <!-- <div>
                    <label for=""> Company Tag Line: </label>
                    <input type="text" placeholder="" class="shadow-none with-border" id="traderCompanyTagline" name="traderCompanyTagline">
                    <p class="text-red-500" id="traderCompanyTaglineValidation"></p>
                  </div> -->
                  <div>
                    <label for=""> Business Website: </label>
                    <input type="text" placeholder="" class="shadow-none with-border" id="businessWebsite" name="businessWebsite">
                    <p class="text-red-500" id="businessWebsiteValidation"></p>
                  </div>
                  <div>
                    <label for=""> Business Email Address: </label>
                    <input type="text" placeholder="" class="shadow-none with-border" id="businessEmailAddress" name="businessEmailAddress">
                    <p class="text-red-500" id="businessEmailAddressValidation"></p>
                  </div>
                  <div>
                    <label for=""> Business Contact Number: </label>
                    <input type="text" placeholder="" class="shadow-none with-border" id="businessContactNumber" name="businessContactNumber">
                    <p class="text-red-500" id="businessContactNumberValidation"></p>
                  </div>
                  <label for=""> Social Media / Messaging App </label>
                  <div>
                    <select id="businessSocialMediaContactType" name="businessSocialMediaContactType" class="shadow-none selectpicker with-border ">
                      <option value="1">Viber</option>
                      <option value="2">Wechat</option>
                      <option value="3">Whatsapp</option>
                    </select>
                  </div>
                  <label for=""> Mobile Number </label>
                  <div>
                    <input type="text" placeholder="Mobile Number" class="shadow-none with-border" id="businessSocialMediaContactNumber" name="businessSocialMediaContactNumber">
                    <p class="text-red-500" id="businessSocialMediaContactNumberValidation"></p>
                  </div>
                  <div>
                    <label for="">Business Country Location: </label>
                    <select id="businessCountryLocation" name="businessCountryLocation" class="shadow-none selectpicker with-border ">
                    </select>
                    <p class="text-red-500" id="businessCountryLocationValidation"></p>
                  </div>
                  <div>
                    <label for="">Business States Location: </label>
                    <select id="businessStatesLocation" name="businessStatesLocation" class="shadow-none selectpicker with-border ">
                    </select>
                    <p class="text-red-500" id="businessStatesLocationValidation"></p>
                  </div>
                  <div>
                    <label for="">Business City Location: </label>
                    <select id="businessCityLocation" name="businessCityLocation" class="shadow-none selectpicker with-border ">
                    </select>
                    <p class="text-red-500" id="businessCityLocationValidation"></p>
                  </div>
                  <div>
                    <label for=""> Languages of communication: </label>
                    <select class="selectpicker" id="language" name="language" multiple>
                    </select>
                    <p class="text-red-500" id="languageValidation"></p>
                  </div>
                </div>
                <!-- =============================  -->
                <!-- Company Details Display - END  -->
                <!-- =============================  -->
              </div>
            </div>
            <!-- ===============================  -->
            <!-- Company Details Display - END  -->
            <!-- ===============================  -->

            <!-- ===============================  -->
            <!-- Company Representative - START  -->
            <!-- ===============================  -->
            <div class="lg:w-2/4 w-full" id="">
              <!-- <div class="card mb-2" style="height: 1160px;"> -->
              <!-- <div class="card mb-2" id="companyRepresentativeDiv"> -->
              <div class="bg-white rounded-md lg:shadow-md shadow">
                <div class="border-b flex items-center justify-between p-4">
                  <div>
                    <h2 class="text-lg font-semibold">Representative
                      <a class="hover:border-gray-100" href="#">
                        <span class="icon-feather-info" style="font-size: 16px; font-family: inherit" uk-tooltip="title: Representative Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat. Duis aute irure dolor in reprehenderit in voluptate velit esse cillum dolore eu fugiat nulla pariatur. Excepteur sint occaecat cupidatat non proident, sunt in culpa qui officia deserunt mollit anim id est laborum.; pos: right">
                        </span>
                      </a>
                    </h2>
                  </div>
                  <div class="text-right">
                  </div>
                </div>
                <div class="flex-1 space-y-4 p-6">
                  <div>
                    <label for=""> First Name </label>
                    <input type="text" placeholder="" class="shadow-none with-border" id="firstName" name="firstName">
                    <p class="text-red-500" id="firstNameValidation"></p>
                  </div>
                  <div>
                    <label for=""> Last Name </label>
                    <input type="text" placeholder="" class="shadow-none with-border" id="lastName" name="lastName">
                    <p class="text-red-500" id="lastNameValidation"></p>
                  </div>
                  <div>
                    <label for=""> Middle Name </label>
                    <input type="text" placeholder="" class="shadow-none with-border" id="middleName" name="middleName">
                    <p class="text-red-500" id="middleNameValidation"></p>
                  </div>
                  <div>
                    <label for="">Country of Residence: </label>
                    <select id="country" name="country" class="shadow-none selectpicker with-border ">
                    </select>
                    <p class="text-red-500" id="countryValidation"></p>
                  </div>
                  <div>
                    <label for=""> States of Residence: </label>
                    <select id="states" name="states" class="shadow-none selectpicker with-border ">
                    </select>
                    <p class="text-red-500" id="statesValidation"></p>
                  </div>
                  <div>
                    <label for=""> City of Residence: </label>
                    <select id="city" name="city" class="shadow-none selectpicker with-border ">
                    </select>
                    <p class="text-red-500" id="cityValidation"></p>
                  </div>
                  <label for=""> Social Media / Messaging App (Optional) </label>
                  <div>
                    <select id="personalSocialMediaContactType" name="personalSocialMediaContactType" class="shadow-none selectpicker with-border ">
                      <option value="1">Viber</option>
                      <option value="2">Wechat</option>
                      <option value="3">Whatsapp</option>
                    </select>
                  </div>
                  <label for=""> Mobile Number (Optional)</label>
                  <div>
                    <input type="text" placeholder="Mobile Number" class="shadow-none with-border" id="personalSocialMediaContactNumber" name="personalSocialMediaContactNumber">
                    <p class="text-red-500" id="personalSocialMediaContactNumberValidation"></p>
                  </div>
                  <div>
                    <label for=""> Representative Email Address (same as Registration/Login email): </label>
                    <input type="text" placeholder="" class="shadow-none with-border" id="emailAddress" name="emailAddress">
                    <p class="text-red-500" id="emailAddressValidation"></p>
                  </div>
                  <div>
                    <label for="">Password</label>
                    <input type="password" placeholder="" class="shadow-none with-border" id="password" name="password">
                    <p class="text-red-500" id="passwordValidation"></p>
                  </div>
                  <div>
                    <label for="">Confirm Password</label>
                    <input type="password" placeholder="" class="shadow-none with-border" id="confirmPassword" name="confirmPassword">
                    <p class="text-red-500" id="confirmPasswordValidation"></p>
                  </div>
                  <div id="hashedPasswordInput">
                    <label for=""> Hashed Password: </label>
                    <input type="text" placeholder="" class="shadow-none with-border" id="hashedPassword" name="hashedPassword">
                  </div>
                  <div id="plainPasswordInput">
                    <label for=""> Plain Password: </label>
                    <input type="text" placeholder="" class="shadow-none with-border" id="plainPassword" name="plainPassword">
                  </div>
                </div>
                <div class="bg-gray-10 p-6 pt-0 flex justify-end space-x-3">
                  <button class="p-2 px-4 rounded bg-gray-50 text-red-500"> Cancel </button>
                  <button type="button" class="button bg-blue-700" id="btnRegistration"> Save </button>
                </div>
              </div>
            </div>
            <!-- ===============================  -->
            <!-- Company Representative - END  -->
            <!-- ===============================  -->
          </div>


        </form>
      </div>
    </div>


  </div>
  <%- include('../includes/scripts/index.ejs') %>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/limonte-sweetalert2/11.3.0/sweetalert2.min.js"></script>
  <script src="assets/js/medium-scale-company-registration-countries.js"></script>
  <script src="assets/js/medium-scale-company-registration.js"></script>
  <script src="assets/js/medium-scale-company-registration-validation.js"></script>
  <script src="assets/js/medium-scale-company-registration-upload-medias.js"></script>
  <script src="assets/js/registration-email-verification.js"></script>
  <script src="assets/js/registration-location-replace.js"></script>
  


</body>

</html>